<h1>Listing aws_regions</h1>


<div id="aws_forum_sync">
  <button id="aws_forum_sync_button" class="btn btn-info"> Sync with AWS Forums IP Range Post </button>
  <div id="aws_forum_sync_status"></div>
</div>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @aws_regions.each do |aws_region| %>
  <tr>
    <td><%= link_to aws_region.name, aws_region %></td>
  </tr>
<% end %>
</table>

